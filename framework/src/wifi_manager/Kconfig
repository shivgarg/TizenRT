config WIFI_MANAGER
    bool "Enable Wi-Fi Manager"
    default n
    ---help---
        Easy APIs for applications to use and control Wi-Fi features

if WIFI_MANAGER
source "$EXTERNALDIR/slsi_wifi/Kconfig"
source "$EXTERNALDIR/wpa_supplicant/Kconfig"
endif #WIFI_MANAGER
